{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["apps/**/.next/**", "apps/**/dist/**"]
    },
    "lint": {},
    "test": {}
  },
  "globalDependencies": [
    "turbo-cache-key.json",
    "package.json",
    "apps/web/**",
    "apps/api/**"
  ],
  "envMode": "loose"
}
